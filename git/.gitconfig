[user]
	email = josef.gottlander@gmail.com
	name = Josef Gottlander
[alias]
	hist = log --all --decorate --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	tree = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)' --all
[core]
	editor = vim
[push]
	default = simple
